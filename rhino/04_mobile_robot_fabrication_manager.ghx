                    <item name="CodeInput" type_name="gh_string" type_code="10">import os
from compas.robots import LocalPackageMeshLoader

#loaders

ur_path = os.path.join(ur_data_path, "robot_description")
ur_packages = ["ur_description", "ur_e_description", "ur_end_effectors", "ur_setups"]
ur_loaders = [LocalPackageMeshLoader(ur_path, pck) for pck in ur_packages]

rbvogui_packages = ["rbvogui_description"]
rbvogui_loaders = [LocalPackageMeshLoader(rbvogui_data_path, pck) for pck in rbvogui_packages]

ewellix_packages = ["ewellix_description"]
ewellix_loaders = [LocalPackageMeshLoader(repositories_path, pck) for pck in ewellix_packages]

