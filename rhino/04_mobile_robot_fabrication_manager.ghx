                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st
from fabrication_manager.fabrication import FabricationManager

if init:
    fabrication = FabricationManager(server)
    fabrication.parallelize = parallelize
    st["fabrication"] = fabrication

fabrication = st["fabrication"]

if set_tasks:
    print "setting tasks"    
    fabrication.clear_tasks()
    fabrication.set_tasks(tasks)

if start:
    #if robot.is_connected(): #and arduino.is_connected():
    print "starting process"
    fabrication.start()

if stop:
    print "stopping process"
    fabrication.stop()
    
if reset:
    print "resetting process"
    fabrication.reset()

if next_task:
    t = fabrication.get_next_task()
    task_state_set(True, t)
    
if reset_task:
    t = fabrication.get_next_task()
    task_state_set(False, t)

if prev_task:
    t = fabrication.get_next_task()
    task_state_set(False, t)
    t = fabrication.tasks[fabrication.current_task_key-1]
    task_state_set(False, t)
    fabrication.get_next_task()

def task_state_set(state, t):
    t.is_completed = state
    t.is_running = False

server = fabrication.server

