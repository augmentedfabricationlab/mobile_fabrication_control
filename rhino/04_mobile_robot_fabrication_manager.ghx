                    <item name="CodeInput" type_name="gh_string" type_code="10">import rhinoscriptsyntax as rs
from compas.geometry import Frame
from compas.geometry import Translation
from compas_ghpython.utilities import draw_frame

if choice=="measured":
    ur_base_frame_rhino_local_tf = Frame(ur_base_plane_rhino_local_tf.Origin, 
                                         ur_base_plane_rhino_local_tf.XAxis, 
                                         ur_base_plane_rhino_local_tf.YAxis)
    T = Translation.from_vector(ur_base_frame_rhino_local_tf.zaxis*(vertical_axis_height+vertical_axis_correction))
    ur_base_frame_rhino_local_tf = ur_base_frame_rhino_local_tf.transformed(T)
elif choice=="initial":
    ur_base_frame_rhino_local_tf = Frame(ur_base_plane_init.Origin, 
                                         ur_base_plane_init.XAxis, 
                                         ur_base_plane_init.YAxis)
    T = Translation.from_vector(ur_base_frame_rhino_local_tf.zaxis*(vertical_axis_height+vertical_axis_correction))
    ur_base_frame_rhino_local_tf = ur_base_frame_rhino_local_tf.transformed(T)

elif choice =="default":
    ur_base_frame_rhino_local_tf = Frame([0.275, 0.0, 1.105], [-0.707, 0.707, 0.0], [-0.707, -0.707,0.0])
    T = Translation.from_vector(ur_base_frame_rhino_local_tf.zaxis*(vertical_axis_height+vertical_axis_correction))
    ur_base_frame_rhino_local_tf = ur_base_frame_rhino_local_tf.transformed(T)


ur_base_plane = draw_frame(ur_base_frame_rhino_local_tf)</item>
