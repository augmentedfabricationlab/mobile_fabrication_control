                    <item name="CodeInput" type_name="gh_string" type_code="10">import math
import Grasshopper
from compas.robots import Joint
from compas_fab.robots import Configuration

if set_joint_positions:
    j = 0
    for i in range(ghenv.Component.Params.Input.Count):
        nslider = ghenv.Component.Params.Input[i].Sources[0]
        if nslider.GetType() == Grasshopper.Kernel.Special.GH_NumberSlider:
            nslider.TrySetSliderValue(math.degrees(joint_positions[j]))
            j += 1

if robot:
    joint_positions = [lift, a1, a2, a3, a4, a5, a6]
    joint_types = robot.get_configurable_joint_types()
    joint_names = robot.get_configurable_joint_names()
#    joint_types = [type for type in joint_types if type == Joint.REVOLUTE]
    
    for i, joint_type in enumerate(joint_types):
        if joint_type == Joint.REVOLUTE:
            joint_positions[i] = math.radians(joint_positions[i])
        else:
            joint_positions[i] = joint_positions[i]
    
    full_configuration = Configuration(joint_positions, joint_types, joint_names)
    
