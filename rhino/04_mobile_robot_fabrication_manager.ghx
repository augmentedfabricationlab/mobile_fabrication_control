                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st
from ur_fabrication_control.direct_control.fabrication_process import URTask
from extruder_control.fabrication import EXTTask
from ur_fabrication_control.direct_control import URScript
from compas.geometry import Frame, Transformation
from compas_ghpython.utilities import ghtree_to_list, draw_frame

ur_ip = st["ur_ip"]
ur_port = st["ur_port"]
server_ip = st["server_ip"]
server_port = st["server_port"]

def tasks_from_planes(planestree, start_ind=0):
    tasks = []
    for planes in planestree:
        tasks.append(urtask_from_planes(planes, start_ind+len(tasks)))
    return tasks

def urtask_from_planes(planes, key):
    req_msg = "Task_{}_Completed".format(key)
    if key%2==0:
        planes = list(reversed(planes))
    urscript = urscript_from_planes(planes, key, req_msg)
    new_task = URTask(server, urscript, req_msg, key)
    return new_task

def planes_to_frames(planes):
    print(planes[0])
    return [Frame(plane.Origin, plane.XAxis, plane.YAxis) for plane in planes if plane is not None]

def get_frames_in_RCS(frames):
    return [robot.RCF_in_WCF.to_local_coordinates(frame) for frame in frames]


def urscript_from_planes(planes, pkey, exit_message):
    frames = planes_to_frames(planes)
#    T0_frames = robot.attached_tool.from_tcf_to_t0cf(frames)
    RCS_frames = get_frames_in_RCS(frames)
    
    ur_script = URScript(ur_ip=ur_ip, ur_port=ur_port)
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
#    tool_angle_axis = [0,0,0,0,0,0]
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    ur_script.add_line("textmsg(\"&gt;&gt; TASK {}.\")".format(pkey))
    # Initialize first movement (with accelleration)
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    
    for key, RCS_frame in enumerate(RCS_frames):
        if key == 0:
            ur_script.move_linear(RCS_frame, robot_speed, 0.01)
            node_msg = {"TASK":pkey, "NODE":0}
            ur_script.socket_send_line_string(str(node_msg), socket_name="Feedbackserver")
        else:
            # Send all next frames of the layer (constant speed, no accelleration)
            ur_script.move_process(RCS_frame, robot_speed, 0.01)
            node_msg = {"TASK":pkey, "NODE":key}
            ur_script.socket_send_line_string(str(node_msg), socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")# BS: Closing socket at the end of the loop
    # Signify ready
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    ur_script.socket_send_line_string(exit_message, socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")# BS: Closing socket at the end of the loop
    #Finalize script
    ur_script.end()
    ur_script.generate()
    return ur_script

