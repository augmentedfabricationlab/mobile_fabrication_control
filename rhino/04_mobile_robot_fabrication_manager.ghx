fabrication = st["fabrication"]

if hasattr(fabrication, "fab_thread"):
    if fabrication.fab_thread.is_alive():
        state = "THREAD RUNNING"
    else:
        state = "THREAD STOPPED"
else:
    state = "THREAD NOT STARTED"

num_tasks = len(fabrication.tasks)
if fabrication.current_task_key is not None:
    num_current_task = fabrication.current_task_key
else:
    num_current_task = 0
num_tasks_left = num_tasks-num_current_task
log_messages = fabrication.log_messages

if update:
    update_component(ghenv, 1000)</item>
