import bisect

### calculate less layers (for development only)
n_layers_calcualted = 400
base_crvs = base_crvs[:n_layers_calcualted]
norm_segs = norm_segs[:u_val*n_layers_calcualted]
ond_segs = ond_segs[:u_val*n_layers_calcualted]

new_ond_segs = []

def sortkey1(tuple):
    par = tuple[2]
    return abs(par-0.5)
    
def sortkey2(tuple):
    return tuple[1]

def sortkey3(par):
    parpt = ghcomp.EvaluateCurve(ond, par)[0]
    dis = ghcomp.Distance(parpt, cp.Origin)
    return dis

a = []
b = []

### each layer
for key, base_crv in enumerate(base_crvs):
    
    ### calculate norm domain
    normlen = base_crv.GetLength()
    normdomain = base_crv.Domain
    normdomainlen = normdomain[1] - normdomain[0]
    
    ### calculate spike parameters (crvpars)
    crvpars = []
    
    d0 = min_seg_width_0*normdomainlen/(u_val/2) # minimal length of first group of segments
    d1 = min_seg_width_1*normdomainlen/(u_val/2) # minimal length of second group of segments
    
    s0 = start_t - d0/2 + d0/num_spikes/2 # start parameter of first group of spikes
    s1 = start_t + normdomainlen/u_val - d1/2 + d1/num_spikes/2 # start parameter of second group of spikes
    
    for i in range(u_val):
        if u_val % 2 == 1:
            if i == 0:
                for j in range(num_spikes):
                    d00 = d0/2 + d1/2
                    s00 = start_t - d1/2 + d00/num_spikes/2
                    par00 = s00 + j*d00/num_spikes
                    if par00 &gt; normdomain[1]:
                        par00 -= normdomainlen
                    crvpars.append(par00)
            elif i % 2 == 0:
                for j in range(num_spikes):
                    par0 = s0 + (i/2)*normdomainlen/(u_val/2) + j*d0/num_spikes
                    if par0 &gt; normdomain[1]:
                        par0 -= normdomainlen
                    crvpars.append(par0)
            else:
                for j in range(num_spikes):
                    par1 = s1 + ((i-1)/2)*normdomainlen/(u_val/2) + j*d1/num_spikes
                    if par1 &gt; normdomain[1]:
                        par1 -= normdomainlen
                    crvpars.append(par1)
        else:
            if i % 2 == 0:
                for j in range(num_spikes):
                    par0 = s0 + (i/2)*normdomainlen/(u_val/2) + j*d0/num_spikes
                    if par0 &gt; normdomain[1]:
                        par0 -= normdomainlen
                    crvpars.append(par0)
            else:
                for j in range(num_spikes):
                    par1 = s1 + ((i-1)/2)*normdomainlen/(u_val/2) + j*d1/num_spikes
                    if par1 &gt; normdomain[1]:
                        par1 -= normdomainlen
                    crvpars.append(par1)
            
    crvpars.sort()
    firstond = ond_segs[u_val*key]
    start_p = ghcomp.CurveClosestPoint(firstond.PointAtStart, base_crv)[1]
    index = bisect.bisect_left(crvpars, start_p)
    crvpars = crvpars[index:] + crvpars[:index]
    
    ### calculate spike points and vectors
    crvpts, crvtans, crvans = ghcomp.EvaluateCurve(base_crv, crvpars)
    vecs = ghcomp.CrossProduct(crvtans, rg.Vector3d.ZAxis, True)[0]
    newvecs = [vec*spike_length for vec in vecs]
    
    ### layer plane
    crvZ = crvpts[0].Z
    plane = ghcomp.PlaneNormal(rg.Point3d(0,0,crvZ), rg.Vector3d.ZAxis)
    
    ### each segment
    for i in range(u_val):
        ond = ond_segs[u_val*key+i]
        segpts = crvpts[i*num_spikes:(i+1)*num_spikes]
        segtans = crvtans[i*num_spikes:(i+1)*num_spikes]
        segpars = crvpars[i*num_spikes:(i+1)*num_spikes]
        segvecs = newvecs[i*num_spikes:(i+1)*num_spikes]
        
        ### offset ond
        offset = ghcomp.OffsetCurve(ond, path_width, plane, 0)
        if offset.GetLength() &lt; 0.5*ond.GetLength():
            print("ond offset rebuilt at layer", key, "segment", i)
            count = ond.GetLength()/0.005
            rebuild = ghcomp.RebuildCurve(ond, 2, count, True)
            offset = ghcomp.OffsetCurve(rebuild, path_width, plane, 0)
        
        ### each joint
        for j, pt in enumerate(segpts):
            
            ### get tangents, vectors, parameters
            tan = segtans[j]
            vec = segvecs[j]
            par = segpars[j]
            
            ### generate spike polyline
            normleft = ghcomp.EvaluateCurve(base_crv, par - normdomainlen*spike_width/normlen)[0]
            normright = ghcomp.EvaluateCurve(base_crv, par + normdomainlen*spike_width/normlen)[0]
            tipleft = pt + vec - 0.5*path_width*tan
            tipright = pt + vec + 0.5*path_width*tan
            spike = ghcomp.PolyLine([normleft, tipleft, tipright, normright], False)
            spike = ghcomp.ExtendCurve(spike, 0, 2*layer_width, 2*layer_width)
            spikedomain = ghcomp.ConstructDomain(0,1)
            spike = ghcomp.CurveDomain(spike, spikedomain)[0]
            
            ### calculate intersection (spike polyline X ond offset)
            xpts, xparsoffset, xparsspike = ghcomp.CurveXCurve(offset, spike)
            
            ### rebuild if intersection fails
            if type(xpts) == list:
                rebuildoffset = offset
            else:
                count = offset.GetLength()/0.005
                rebuildoffset = ghcomp.RebuildCurve(offset, 2, count, False)
                xpts, xparsoffset, xparsspike = ghcomp.CurveXCurve(rebuildoffset, spike)
                print("ond offset rebuilt at layer", key, "segment", i, "joint", j)
            
            ### find correct intersections if more than 2
            xlist = zip(xpts, xparsoffset, xparsspike)
            if len(xlist) != 2:
                xlist.sort(key = sortkey1)
                xlist.sort(key = sortkey2)
            unzipped_object = zip(*xlist)
            unzipped_list = list(unzipped_object)
            xpts = unzipped_list[0]
            xparsoffset = unzipped_list[1]
            xparsspike = unzipped_list[2]
            offsetdom = ghcomp.ConstructDomain(xparsoffset[0], xparsoffset[1])
            spikedom = ghcomp.ConstructDomain(xparsspike[0], xparsspike[1])
            
            ### trim spike polyline and ond offset
            spike = ghcomp.SubCurve(spike, spikedom)
            newoffset = ghcomp.SubCurve(rebuildoffset, offsetdom)
            a.append(spike)
            a.append(newoffset)
            ### joint perp frame
            cp = ghcomp.PerpFrame(base_crv, par)
            
            ### intersect ond and cp
            ond_par = ghcomp.CurveXPlane(ond, cp)[1]
            
            ### find correct intersection if more than 1
            if type(ond_par) != list:
                None
            else:
                ond_par.sort(key = sortkey3)
                ond_par = ond_par[0]
            
            ### split ond
            ondlen = ond.GetLength()
            onddomain = ond.Domain
            onddomainlen = onddomain[1] - onddomain[0]
            ond_parleft = ond_par - (onddomainlen*0.5*path_width)/ondlen
            ond_parright = ond_par + (onddomainlen*0.5*path_width)/ondlen
            
            ond_parpt = ond.PointAt(ond_par)
            ond_parptleft = ond.PointAt(ond_parleft)
            ond_parptright = ond.PointAt(ond_parright)
            
            ondsegs = ghcomp.Shatter(ond, [ond_parleft, ond_parright])
            ondleft = ondsegs[0]
            ondright =ondsegs[-1]
            
            ### split ond offset
            newoffset = ghcomp.RebuildCurve(newoffset, 2, 10, True)
            off_parpt, off_par, distance = ghcomp.CurveClosestPoint(ond_parpt, newoffset)
            offlen = newoffset.GetLength()
            offdomain = newoffset.Domain
            offdomainlen = offdomain[1] - offdomain[0]
            off_parleft = off_par - (offdomainlen*0.5*path_width)/offlen
            off_parright = off_par + (offdomainlen*0.5*path_width)/offlen
            off_parptleft = newoffset.PointAt(off_parleft)
            off_parptright = newoffset.PointAt(off_parright)
            
            offsegs = ghcomp.Shatter(newoffset, [off_parleft, off_parright])
            offleft = offsegs[0]
            offright = offsegs[-1]
            
            ### generate mid lines
            mid1 = ghcomp.Line(ond_parptleft, off_parptright)
            mid2 = ghcomp.Line(off_parptleft, ond_parptright)
            
            ### join segments
            spike = ghcomp.FlipCurve(spike, None)[0]
            newond = ghcomp.JoinCurves([ondleft, mid1, offright, spike, offleft, mid2, ondright], True)
            ond = newond
            
        new_ond_segs.append(ond)</item>
