if 'listener_tf_static' in sticky:
    if sticky['listener_tf_static']:
    
        listener_tf_static = sticky['listener_tf_static']
        listener_tf_static.subscribe(receive_message_tf_static)
    
        if 'ros_msg_tf_static' in sticky and sticky['ros_msg_tf_static']:
            #bang = True
            
            ros_msg_tf_static = sticky['ros_msg_tf_static']
            if len(ros_msg_tf_static and ros_msg_tf_static['transforms']):
                
                ### First static transformation from mobile robot base frame to mobile robot base link
                tf0 = ros_msg_tf_static['transforms'][0]
                t0 = tf0['transform']
                #print('robot_base frame to robot_base_link', t0)
                x1, y1, z1 = t0['translation']['x'], t0['translation']['y'], t0['translation']['z']
                
                ## Second static transformation from mobile robot base link to ewellix base link
                tf1 = ros_msg_tf_static['transforms'][2]
                t1 = tf1['transform']
                #print('robot_base_link to ewellix_base_link', t1)
                x2, y2, z2 = t1['translation']['x'], t1['translation']['y'], t1['translation']['z']
                
                print('++ Listening to ROS MSG TF STATIC')

                ## Translations in x, y, z axis from the received static transformations
                T1 = Translation.from_vector(mobile_base_frame_ros.xaxis*(x1+x2))
                T2 = Translation.from_vector(mobile_base_frame_ros.yaxis*(y1+y2))
                T3 = Translation.from_vector(mobile_base_frame_ros.zaxis*(z1+z2))

                T_rbl_ebl = T1*T2*T3
                
                ewellix_base_frame_ros = mobile_base_frame_ros.transformed(T_rbl_ebl)
                ewellix_base_plane_ros = draw_frame(ewellix_base_frame_ros)
        else:
            bang = False
            print('NOT receiving ROS MSG TF STATIC')
    else:
        print('NOT receiving ROS MSG TF STATIC')


if 'listener_robot_lift_pos' in sticky:
    if sticky['listener_robot_lift_pos']:
    
        listener_robot_lift_pos = sticky['listener_robot_lift_pos']
