import Rhino.Geometry as rg
import math

guid = str(ghenv.Component.InstanceGuid)
timer_running_key = "timer_running_" + guid
counter_key = "counter_" + guid

robot_lift_length = 0.533

if stop_timer or timer_running_key not in sticky:
    sticky[timer_running_key] = False
    sticky[counter_key] = 0

if start_timer:
    sticky[timer_running_key] = True
    sticky[counter_key] = 0
    

if sticky[timer_running_key]:
    update_component(ghenv, 1000)
    
    sticky[counter_key] += 1
    print(sticky[counter_key])


def receive_message_tf_static(message):
    #sticky['message'] = str(datetime.datetime.now()) + ': ' + str(message)
    #print("receiving")
    sticky['ros_msg_tf_static'] = message
    
def receive_message_robot_footprint(message):
    #sticky['message'] = str(datetime.datetime.now()) + ': ' + str(message)
    #print("receiving")
    sticky['ros_msg_robot_footprint'] = message

def receive_message_robot_lift_pos(message):
    #sticky['message'] = str(datetime.datetime.now()) + ': ' + str(message)
    #print("receiving")
    sticky['ros_msg_robot_lift_pos'] = message
    
def receive_message_robot_battery_data(message):
    #sticky['message'] = str(datetime.datetime.now()) + ': ' + str(message)
