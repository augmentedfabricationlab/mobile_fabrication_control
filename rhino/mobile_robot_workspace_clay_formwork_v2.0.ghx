        if 'ros_msg_robot_footprint' in sticky and sticky['ros_msg_robot_footprint']:
            #bang = True
            
            ros_msg_robot_footprint = sticky['ros_msg_robot_footprint']
            if len(ros_msg_robot_footprint):
                robot_base_footprint = ros_msg_robot_footprint
                p = robot_base_footprint['pose']['pose']
                #print('robot_base_footprint', p)
                mobile_base_frame_ros = Frame.from_quaternion([p['orientation']['w'],p['orientation']['x'], p['orientation']['y'], p['orientation']['z']], point = [p['position']['x'], p['position']['y'], p['position']['z']])
                mobile_base_plane_ros = draw_frame(mobile_base_frame_ros)
