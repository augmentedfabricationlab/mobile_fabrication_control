class Chunk():
    def __init__(self, id):
        self.id = id
    @property
    def v(self):
        v = self.id//u_val
        return v
    @property
    def u(self):
        u = self.id%u_val
        return u
    @property
    def layers(self):
        if self.id &lt; u_val/2:
            start_layer = 0
            end_layer = seg_lrs
        else:
            if self.u &lt; u_val/2:
                start_layer = (self.v-1)*seg_lrs*2 + seg_lrs
                end_layer = self.v*seg_lrs*2 + seg_lrs
            else:
                start_layer = self.v*seg_lrs*2
                end_layer = (self.v+1)*seg_lrs*2
        layers = (start_layer, end_layer)
        return layers
    @property
    def layer_count(self):
        count = self.layers[1] - self.layers[0] + 1
        return count
    @property
    def is_valid(self):
        if self.layers[0] &gt;= n_layers_calculated:
            return False
        else:
            return True
    @property
    def segs(self):
        new_u = hseq[self.u]
        segids = []
        for i in range(self.layers[0], self.layers[1]):
            if i &lt; n_layers_calculated:
                segids.append(6*i+new_u)
            else:
                break
        segs = []
        for key, id in enumerate(segids):
            segs.append(Segment(id, self, key))
        return segs
    @property
    def boundary(self):
        normstarts = [seg.norm.PointAtStart for seg in self.segs]
        normends = [seg.norm.PointAtEnd for seg in self.segs]
        btmnorm = self.segs[0].norm
        topnorm = self.segs[-1].norm
        plleft = ghcomp.PolyLine(normstarts, False)
        plright = ghcomp.PolyLine(normends, False)
        plright.Reverse()
        btmnorm.Reverse()
        boundary = ghcomp.JoinCurves([plleft, topnorm, plright, btmnorm], True)
        return boundary
    @property
    def center(self):
        btmnorm = self.segs[0].norm
        topnorm = self.segs[-1].norm
        ct = (ghcomp.CurveMiddle(btmnorm) + ghcomp.CurveMiddle(topnorm))/2
        return ct
            
class Segment():
