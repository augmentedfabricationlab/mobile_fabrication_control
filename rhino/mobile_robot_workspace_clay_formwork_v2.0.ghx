ur_packages = ["ur_description", "ur_e_description", "ur_end_effectors", "ur_setups"]
ur_loaders = [LocalPackageMeshLoader(ur_path, pck) for pck in ur_packages]

rbvogui_packages = ["rbvogui_description"]
rbvogui_loaders = [LocalPackageMeshLoader(rbvogui_data_path, pck) for pck in rbvogui_packages]

ewellix_packages = ["ewellix_description"]
ewellix_loaders = [LocalPackageMeshLoader(repositories_path, pck) for pck in ewellix_packages]

sensors_packages = ["robotnik_sensors"]
sensors_loaders = [LocalPackageMeshLoader(repositories_path, pck) for pck in sensors_packages]

loaders = ur_loaders + rbvogui_loaders + ewellix_loaders + sensors_loaders

#files
urdf_filename = os.path.join(repositories_path, r"mobile_fabrication_control\data\rbvogui_xl_lift_ur10e.urdf")
srdf_filename = os.path.join(repositories_path, r"mobile_fabrication_control\data\rbvogui.srdf")

assert os.path.isfile(urdf_filename)
assert os.path.isfile(srdf_filename)</item>
