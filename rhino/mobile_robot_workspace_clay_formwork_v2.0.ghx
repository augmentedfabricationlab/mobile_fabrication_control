                    <item name="CodeInput" type_name="gh_string" type_code="10">import time
import logging
import datetime
import Grasshopper
from roslibpy import Topic
from scriptcontext import sticky

LOGGER = logging.getLogger('roslibpy')

robot_lift_pos = 'listener_robot_lift_pos'
robot_base_footprint = 'listener_robot_base_footprint'
tf_static = 'listener_tf_static'
robot_battery_data = 'listener_robot_battery_data'

if subscribe_to_robot_lift_topic:
    if ros_client and ros_client.is_connected:
        print("subscribed to /robot/ewellix_driver/lift/pos topic")
        listener = Topic(ros_client, '/robot/ewellix_driver/lift/pos', 'std_msgs/Float64')
        sticky[robot_lift_pos] = listener
    else:
        sticky[robot_lift_pos] = None
        print("NOT subscribed to /robot/ewellix_driver/lift/pos topic")
else:
    sticky[robot_lift_pos] = None

if subscribe_to_robotnik_base_topic:
    if ros_client and ros_client.is_connected:
        print("subscribed to /robot/robotnik_base_control/odom topic")
        listener = Topic(ros_client, '/robot/robotnik_base_control/odom', 'nav_msgs/Odometry')
        sticky[robot_base_footprint] = listener
    else:
        sticky[robot_base_footprint] = None
        print("NOT subscribed to /robot/robotnik_base_control/odom topic")
else:
    sticky[robot_base_footprint] = None

if subscribe_to_static_tf_topic:
    if ros_client and ros_client.is_connected:
        print("subscribed to /tf_static topic")
        listener = Topic(ros_client, '/tf_static', 'tf2_msgs/TFMessage')
        sticky[tf_static] = listener
        #print sticky[tf_static]
    else:
        sticky[tf_static] = None
        print("NOT subscribed to /tf_static topic")
else:
    sticky[tf_static] = None
    
if subscribe_to_battery_data:
    if ros_client and ros_client.is_connected:
        print("subscribed to /robot/battery_estimator/data topic")
        listener = Topic(ros_client, '/robot/battery_estimator/data', '/robotnik_msgs/BatteryStatus')
        sticky[robot_battery_data] = listener
        #print sticky[robot_battery_data]
    else:
        sticky[robot_battery_data] = None
        print("NOT subscribed to /robot/battery_estimator/data topic")
else:
    sticky[robot_battery_data] = None

