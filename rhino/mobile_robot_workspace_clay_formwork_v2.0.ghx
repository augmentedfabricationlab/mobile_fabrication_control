                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Frame, Transformation
from compas_ghpython import draw_frame


if map_frame_ros:
    
    if map_plane_rhino:
        map_frame_rhino = Frame(map_plane_rhino.Origin, map_plane_rhino.XAxis, map_plane_rhino.YAxis)
        
        T_global = Transformation.from_frame_to_frame(map_frame_ros, map_frame_rhino)
        
        print('++ RCF received from ROS')
        print(T_global)
    
    else:
        print('++ NO WCF provided')
    
else:
    print('++ NOT receiving RCF')


if ur_base_frame_ros:
    ur_base_frame_rhino_global_tf = ur_base_frame_ros.transformed(T_global)
    mobile_base_frame_rhino_global_tf = mobile_base_frame_ros.transformed(T_global)
    
    ur_base_plane_rhino_global_tf = draw_frame(ur_base_frame_rhino_global_tf)
    mobile_base_plane_rhino_global_tf = draw_frame(mobile_base_frame_rhino_global_tf)

else:
    print('NO ROS Base Coordinate Frame received')
    

