def segment_range(start, stop, step):
    count = 0
    while True:
        temp = float(start + count * step)
        if temp &gt; stop:
            break
        yield temp
        count += 1 

def segment_range_loop(start, domain, div):
    step = (domain[1]-domain[0])/div
    count = 0
    while True:
        temp = float(start + count * step)
        if count == div:
            break
        if temp &gt; domain[1]:
            temp -= domain[1]
            temp += domain[0]
        elif temp &lt; domain[0]:
            temp += domain[1]
            temp -= domain[0]
        yield temp
        count += 1 

