                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math
from scriptcontext import sticky as st


from compas.geometry import Frame
from compas.geometry import Transformation
from compas.geometry import Rotation
from compas.geometry import Translation
from compas.geometry import Point

from compas_ghpython import draw_frame

from am_information_model.model import AMModel
from am_information_model.model import Layer

import Rhino.Geometry as rg


# initialize an empty assembly class
am_model = AMModel()

cp = centerplane
cf = Frame(cp.Origin, cp.XAxis, cp.YAxis)

for i in range(25): # loop through the number of layers i
    T = Translation.from_vector([0,0,layer_height*i])
    cft = cf.transformed(T) #transforms the origin plane

    T1 = Transformation.from_frame_to_frame(cf, cft)
    # create new instances of the element
    new_layer = layer.transformed(T) #returns a deep copy of the initial element at the new location of the frame
    print(new_layer.node(0).frame)
    am_model.add_layer(new_layer)
