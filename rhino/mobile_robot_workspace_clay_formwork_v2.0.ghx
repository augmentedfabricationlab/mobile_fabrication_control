    vec_lower_right_OG = vec_lower_right_OD*mid_distance/out_distance
    vec_lower_left_OC = vec_lower_left_OD*in_distance/out_distance
    vec_lower_right_OC = vec_lower_right_OD*in_distance/out_distance
