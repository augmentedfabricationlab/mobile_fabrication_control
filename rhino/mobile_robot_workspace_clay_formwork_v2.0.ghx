                    <item name="CodeInput" type_name="gh_string" type_code="10">import math
import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import ghpythonlib.components as ghcomp
from compas.geometry import Frame
from compas.geometry import Vector

out_crc_pts = []
in_crc_pts = []
rb_crc_pts = []
for i in range(3):
    pt_outer_crc = [out_crc_dist*math.cos(i*2*math.pi/3), out_crc_dist*math.sin(i*2*math.pi/3),0]
    out_crc_pts.append(rg.Point3d(*pt_outer_crc))
    pt_inner_crc = [inner_crc_dist*math.cos(i*2*math.pi/3+(1/3)*math.pi), inner_crc_dist*math.sin(i*2*math.pi/3+(1/3)*math.pi),0]
    in_crc_pts.append(rg.Point3d(*pt_inner_crc))
    pt_rebar = [rebar_offset*math.cos(i*2*math.pi/3), rebar_offset*math.sin(i*2*math.pi/3),0]
    rb_crc_pts.append(rg.Point3d(*pt_rebar))

out_crcs = [rg.Circle(pt, out_crc_radius) for pt in out_crc_pts]
rebar_crv = [rg.Circle(pt, rebar_size/2) for pt in rb_crc_pts]

lines1= [rg.Line(out_crc_pts[i], in_crc_pts[i]) for i in range(len(out_crc_pts))]
lines2= [rg.Line(out_crc_pts[i], in_crc_pts[i-1]) for i in range(len(out_crc_pts))]

int_pts = ghcomp.CurveXLine(out_crcs, lines1)[0]
vecs = [rg.Line(in_crc_pts[i], int_pts[i*2]) for i in range(3)]
in_crcs = [rg.Circle(pt, vecs[0].Length) for pt in in_crc_pts]

lines = lines1+lines2
bsrf = ghcomp.BoundarySurfaces(lines)

split_out_crc = ghcomp.SplitwithBrep(out_crcs, bsrf)[0]
split_in_crc = ghcomp.SplitwithBrep(in_crcs, bsrf)[0]
split_out_crc_len = ghcomp.Length(split_out_crc)
split_in_crc_len = ghcomp.Length(split_in_crc)

out_crc_segs = ghcomp.SortList(split_out_crc_len, split_out_crc)[1]
in_crc_segs = ghcomp.SortList(split_in_crc_len, split_in_crc)[1]

crv_segs = []
for i in range(3):
    crv_segs.append(out_crc_segs[i+3])
    crv_segs.append(in_crc_segs[i])

base_crv_raw = ghcomp.JoinCurves(crv_segs, False)
base_crv = ghcomp.OffsetCurve(base_crv_raw, 0.01,corners=0)
base_crv_tstart = ghcomp.CurveClosestPoint(rg.Point3d(out_crc_dist+out_crc_radius,0,0), base_crv)[1]
