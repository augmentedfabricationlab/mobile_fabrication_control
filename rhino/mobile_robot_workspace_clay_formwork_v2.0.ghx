                vertical_lift_pos = ros_msg_robot_lift_pos['data']
                T_ebl_etl = Transformation.from_frame(Frame.from_quaternion([1.0,0.0,0.0,0.0], point=[0.0,0.0,robot_lift_length + vertical_lift_pos]))
                T_static = T_rbl_ebl*T_ebl_etl
                ur_base_frame_ros = ewellix_base_frame_ros.transformed(T_ebl_etl)
                R = Rotation.from_axis_and_angle(ur_base_frame_ros.zaxis, math.radians(135), point=ur_base_frame_ros.point)
                ur_base_frame_ros.transform(R)
