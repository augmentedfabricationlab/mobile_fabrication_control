                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math
from compas.geometry import Frame, Transformation, Rotation
from compas_fab.robots import Configuration
from ur_fabrication_control.kinematics.ur_kinematics import inverse_kinematics
from ur_fabrication_control.kinematics.ur_params import ur_params
from compas_ghpython import draw_frame

robot.lift_height = lift

frame_WCS = Frame(plane_WCS.Origin, plane_WCS.XAxis, plane_WCS.YAxis)

# transform frame to robot coordinate system
frame_RCS = frame_WCS.transformed(robot.transformation_WCF_RCF())

if robot.attached_tool:
    tool0_RCS = robot.from_tcf_to_t0cf([frame_RCS])[0]
else:
    tool0_RCS = frame_RCS

plane_tool0_RCS = draw_frame(tool0_RCS)
plane_RCS = draw_frame(frame_RCS)

# calculate solutions to frame
ur_params = ur_params["ur10e"] # get the parameters according to the robot model
print ur_params
solutions = inverse_kinematics(tool0_RCS, ur_params)
print solutions

if not len(solutions):
    joint_values = [0, 0, 0, 0, 0, 0, 0]
    configuration = Configuration.from_revolute_values(joint_values)
else:
    joint_values = [lift] + solutions[idx]
    configuration = [Configuration.from_revolute_values(joint_values)]

print(configuration)
