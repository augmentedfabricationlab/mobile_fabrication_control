from compas.robots import LocalPackageMeshLoader

#loaders

ur_path = os.path.join(ur_data_path, "robot_description")
ur_packages = ["ur_e_description"]
ur_loaders = [LocalPackageMeshLoader(ur_path, pck) for pck in ur_packages]

mobile_robot_path = os.path.join(mobile_robot_data_path, "rbvogui_xl_lift_ur10e_description")
mobile_robot_packages = ["rbvogui_description", "ewellix_description", "robotnik_sensors"]
mobile_robot_loaders = [LocalPackageMeshLoader(mobile_robot_path, pck) for pck in mobile_robot_packages]

loaders = ur_loaders + mobile_robot_loaders

#files
if wheel_type=="indoor":
    urdf_filename = os.path.join(mobile_robot_data_path, r"rbvogui_xl_lift_ur10e_indoor.urdf")
if wheel_type == "outdoor":
