import Grasshopper
from compas.robots import Joint
from compas_fab.robots import Configuration

if set_joint_positions:
    j = 0
    for i in range(ghenv.Component.Params.Input.Count):
        nslider = ghenv.Component.Params.Input[i].Sources[0]
        if nslider.GetType() == Grasshopper.Kernel.Special.GH_NumberSlider:
            nslider.TrySetSliderValue(math.degrees(joint_positions[j]))
            j += 1

if robot:
    joints = robot.get_configurable_joints()
    joint_types = [joint.type for joint in joints]
    joint_names = [joint.name for joint in joints]
    
    joint_positions = [a1, a2, a3, a4, a5, a6]
    joint_positions = [math.radians(position) for position in joint_positions]
    
    if len(joints) == 7:
        joint_positions = [lift] + joint_positions
    
    print joint_types, joint_names, joint_positions
    full_configuration = Configuration(joint_positions, joint_types, joint_names)
    
print(full_configuration)
